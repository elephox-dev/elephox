{
    "name": "elephox/elephox",
    "description": "A new Elephox application.",
    "type": "project",
    "require": {
        "php": "^8.1 <8.3",
        "elephox/builder-doctrine": "^0.2",
        "elephox/builder-whoops-handler": "^0.1",
        "elephox/builder-whoops-middleware": "^0.1",
        "elephox/framework": "^0.5",
        "nunomaduro/collision": "^6.2"
    },
    "require-dev": {
        "elephox/plane": "^2.0",
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env.local') || copy('.env.local.example', '.env.local');\""
        ]
    }
}
