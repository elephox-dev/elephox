{
    "name": "elephox/elephox",
    "description": "A new Elephox application.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1 <8.3",
        "elephox/framework": "self.version",
        "filp/whoops": "^2.14"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../framework"
        }
    ],
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
