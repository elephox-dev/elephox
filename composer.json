{
    "name": "elephox-dev/elephox",
    "description": "A new Elephox application.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-mysqli": "*",
        "elephox/framework": "0.x-dev",
        "filp/whoops": "^2.14"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Database\\": "database/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../framework"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    }
}
